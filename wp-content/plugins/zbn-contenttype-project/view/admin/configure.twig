<div class="wrap">
    {% if notice %}
        <div class="notice {{notice.type}} is-dismissible">
            <p>{{notice.message}}</p>
        </div>
    {% endif %}
    <h2>{{_e("CONTENTTYPE PROJECT",locale)}}</h2>
    <p>{{_e("In this section you can change the default settigns of the plugin",locale)}}</p>
    <form method="post" action="{{formUrl}}" class="validate">
        {{wp_nonce_field('configure', '_contenttypeprojectnonce')}}
        <p class="submit">
            <input name="savedata" id="savedata" class="button button-primary" value="{{_e('Save Changes',locale)}}" type="submit">
        </p>
        {{render_wp_table("Fields_Configure_Table")}}
    </form>
</div>