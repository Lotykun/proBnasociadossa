{% extends "base.twig" %}

{% block content %}
	<div class="content-wrapper wrapper-div-content">
		<article class="post-type-{{post.post_type}}" id="post-{{post.ID}}">
			<section class="article-content">
                            <h1 class="article-h1 contact-page-h1">{{post.title|upper}}</h1>
                            <div class="article-body wrapper-div-content-contact">
                                    {{post.content}}
                            </div>
			</section>
		</article>
	</div><!-- /content-wrapper -->
{% endblock %}
